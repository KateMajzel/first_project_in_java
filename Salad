package restaurant;

import java.util.Objects;

public class Salad extends Dish{

    public Salad(Integer number, String name, Integer price, Integer calories) {
        super(number, name, price, calories);
    }

    @Override
    public String toString() {
        return "Salad " + getNumber() + ". " + getName() + "  " + getPrice() + " USD" + "  " + getCalories() + " calories \n";
    }
}
